import pandas as pd

data = pd.read_csv('Walmart.csv')
(data.head())

X = data[['Store', 'Date', 'Holiday_Flag', 'Temperature', 'Fuel_Price', 'CPI', 'Unemployment']] #features
y = data ['Weekly_Sales'] #target variable. 

import matplotlib.pyplot as plt

